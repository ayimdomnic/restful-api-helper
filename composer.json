    "keywords": ["laravel", "api", "starter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Kubai",
            "email": "sam@ralphowino.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "dingo/api": "1.0.*@dev",
        "tymon/jwt-auth": "0.5.*",
        "illuminate/support": "~5.0",
        "barryvdh/laravel-cors": "^0.7.1"
    },
    "autoload": {
        "psr-4": {
            "Ralphowino\\ApiStarter\\": "src/"
